                  <TextField fx:id="toYearContractsField" layoutX="236.0" layoutY="35.0" prefHeight="25.0" prefWidth="113.0" promptText="den nam" />
                  <Button layoutX="139.0" layoutY="35.0" mnemonicParsing="false" onMouseClicked="#analyzeContractCountInMonths" text="tinh thang" />
                  <Button layoutX="359.0" layoutY="35.0" mnemonicParsing="false" onMouseClicked="#analyzeContractCountInYears" text="tinh nam" />
